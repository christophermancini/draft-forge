{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "markdown": {
    "lineWidth": 120,
    "textWrap": "maintain"
  },
  "includes": ["**/*.md"],
  "excludes": [
    "**/node_modules",
    "**/.git",
    "**/dist",
    "**/build",
    "**/.wrangler",
    "**/.svelte-kit"
  ],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.17.8.wasm"
  ]
}
